<?xml version="1.0" encoding="UTF-8"?>
<form dataType="ETERNOS_FICHA" formType="sheetTemplate" title="Eternos" name="ETERNOS_NOME" theme="dark">
	<import file="styles.xml"/>
	<import file="templates.xml"/>
	
	<tabControl align="client" name="pgcPrincipal">
		<tab title="Frente">
			<import file="frente.xml"/>
		</tab>
		
		<tab title="Caracteristicas">
			<import file="caracteristicas.xml"/>
		</tab>		

		<tab title="Anotações">
			<rectangle class="fundo">
				<textEditor align="client" field="outros.anotacoes" class="transparente"  margins="{left=2, right=2, top=2, bottom=2}"/>
			</rectangle>
		</tab>
	</tabControl>		
</form>